@{
    ViewBag.Title = "Fight";
}
<!DOCTYPE html>
<html ng-app="App" dir="ltr" lang="en-US">
<head>
    <script src="~/Scripts/CustomScripts/UiRouterCrtl.js"></script>
    <title></title>
</head>
<body>
  
    <div data-ng-controller="Fight">

    </div>

    <script>

        var App = angular.module('App', []);

        App.controller('Fight', function ($scope, $http) {
            $http.get('http://localhost:60138/api/fight/').success(function (data) {
                $scope.data = data;
            });
        });
    </script>
</body>
</html>